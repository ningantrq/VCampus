<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" stylesheets="@../../css/ClientCss.css" xmlns="http://javafx.com/javafx/22" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.vcampus.controller.studentManagement.deleteComfirmController">
   <children>
      <VBox prefHeight="400.0" prefWidth="600.0">
         <children>
            <HBox alignment="CENTER" prefHeight="575.0" prefWidth="600.0">
               <children>
                  <Text fill="#ae0707" strokeType="OUTSIDE" strokeWidth="0.0" styleClass="text" text="确认是否删除">
                     <font>
                        <Font size="44.0" />
                     </font>
                  </Text>
               </children></HBox>
            <HBox alignment="CENTER" prefHeight="349.0" prefWidth="600.0">
               <children>
                  <Button fx:id="confirm" mnemonicParsing="false" onMouseClicked="#ClickConfirm" prefHeight="45.0" prefWidth="93.0" text="确认" />
                  <HBox prefHeight="87.0" prefWidth="112.0" />
                  <Button fx:id="cancel" mnemonicParsing="false" onMouseClicked="#ClickCancel" prefHeight="45.0" prefWidth="93.0" text="取消" />
               </children></HBox>
         </children>
      </VBox>
   </children>
</AnchorPane>
